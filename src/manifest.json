{
  "$schema": "https://json.schemastore.org/youtrack-app.json",
  "name": "youtrack-article-actions-hu",
  "title": "YouTrack Cikk akciók",
  "description": "Tudásbázis cikkek másolása és mozgatása",
  "changeNotes": "See https://plugins.jetbrains.com/plugin/25371-youtrack-article-actions/release-notes",
  "version": "1.0.3",
  "icon": "logo.svg",
  "minYouTrackVersion": "2024.3.0",
  "widgets": [
    {
      "key": "article-menu-demo",
      "name": "Copy or Move",
      "indexPath": "index.html",
      "extensionPoint": "ARTICLE_OPTIONS_MENU_ITEM",
      "expectedDimensions": {
        "width": 360,
        "height": 330
      },
      "permissions": ["CREATE_ARTICLE", "UPDATE_ARTICLE"]
    }
  ]
}
